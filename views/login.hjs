<head>
	<meta charset="utf-8">
  <meta name="viewport" content="width=device-width">
  <title>Sign Up</title>

  
  <link rel="stylesheet" href='css/foundation.css'>
  <link rel="icon" type="img/png" href="img/logo.png">
  <link rel='stylesheet' href='css/candy.css'>
  <link rel='stylesheet' href='css/cameleon.css'>

  <script src="js/vendor/custom.modernizr.js"></script>

</head>
<body>

  <!--HEADER, POSITION:FIXED, LARGE ONLY-->
  <div class='fixed hide-for-small' id='head'>
    <div class='row'>
      <div class='large-3 columns' id='number'>
        <h4>(781) 559-4602 </h4>
      </div>
      <div class='large-6 columns' id='signupTitle'>
        <a href="/"><h1>WheelTalks</h1></a>
      </div>
      <div class='large-3 columns' id='fixedNav'>  
        <div id='connect' class='button right success'><a href="/signup">Connect</a></div>
      </div>
    </div>
  </div>

  <!--HEADER, POSITION:RELATIVE, SMALL ONLY-->
    <nav class="top-bar" id='appNav'>
  <ul class="title-area">
        <li class="name">
      <h1><a href="#">WheelTalks</a></h1>
  </ul>
</nav>

<div class='row'>
<div align =  "center"><form method="post" action="/login">
    <fieldset>
    <legend align =  "center">Please Log In</legend>

    <div class="row">
      <div class="large-12 columns">
        <label>ID</label>
        <input type="text" name = "user" id = "user" placeholder="state + plate">
      </div>
    </div>

    <div class="row">
      <div class="large-12 columns">
        <label>Password</label>
        <input type="password" id = "pass" name = "pass" placeholder="password">
      </div>
     </div>

    <div class="large-12 columns">
      <label for="checkbox1"><input type="checkbox" name = "remember-me" id="remember-me" ><span class="custom checkbox"></span> remember me</label>

    </div>

      <div id="button">
                <input class='button' id='submitButton' type="submit" value="Submit" />
            </div>
            
  </fieldset>
</form>

      </div>
    </div>


    
    
  <script>
  document.write('<script src=' +
  ('__proto__' in {} ? 'js/vendor/zepto' : 'js/vendor/jquery') +
  '.js><\/script>')
  </script>
  
  <script src="/js/foundation.min.js"></script>
  <script type="text/javascript" src='/js/raspberry.js'></script>
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.js"></script>
  
  <script>
    $(document).foundation();
  </script>
</body>
</html>
