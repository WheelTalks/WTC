  <link rel="stylesheet" href="css/signup.css">
  <link rel='stylesheet' href='css/candy.css'>
 
  <script src="js/vendor/custom.modernizr.js"></script>

</head>
<body>

  <!--HEADER FOR BOTH LARGE AND SMALL-->
  <div class='fixed contain-to-grid' id='navWrapper'>
    <nav class="top-bar" id='appNav'>
      <ul class="title-area">
        <li class="name">
          <a id='navName' href="/">WheelTalks</a>
        </li>
        <li class="toggle-topbar menu-icon">
          <a href="#"><span></span></a>
        </li>
      </ul>
      <section class="top-bar-section">
        <ul class="right" id='navRight'>
          <li class="divider show-for-small" >
            <li class='active'>
              <a href='/loginPage'>Log In</a>
            </li>
          </li>
        </ul>
      </section>
    </nav>
  </div>
 
   <div class='row'>
    <div class='large-10 columns small-centered' id='signUpForm'>
      <fieldset>
        <legend><h5 id="title2" class="small-centered columns">See what people are sending you</h5></legend>
        <div class='large-12 columns'>
          <form data-abide form method="post" action="/new" id="signupForm">
            <div class='row' id='lplate'>
              <div class='columns' id="state">
                  <select name="licenseplate" id="stateInput" required pattern="[a-zA-Z]+"><option value="--">--</option> <option value="al">AL</option> <option value="ak">AK</option> <option value="az">AZ</option> <option value="ar">AR</option> <option value="ca">CA</option> <option value="co">CO</option> <option value="ct">CT</option> <option value="de">DE</option> <option value="fl">FL</option> <option value="ga">GA</option> <option value="hi">HI</option> <option value="id">ID</option> <option value="il">IL</option> <option value="in">IN</option> <option value="ia">IA</option> <option value="ks">KS</option> <option value="ky">KY</option> <option value="la">LA</option> <option value="me">ME</option> <option value="md">MD</option> <option value="ma">MA</option> <option value="mi">MI</option> <option value="mn">MN</option> <option value="ms">MS</option> <option value="mo">MO</option> <option value="mt">MT</option> <option value="ne">NE</option> <option value="nv">NV</option> <option value="nh">NH</option> <option value="nj">NJ</option> <option value="nm">NM</option> <option value="ny">NY</option> <option value="nc">NC</option> <option value="nd">ND</option> <option value="oh">OH</option> <option value="ok">OK</option> <option value="or">OR</option> <option value="pa">PA</option> <option value="ri">RI</option> <option value="sc">SC</option> <option value="sd">SD</option> <option value="tn">TN</option> <option value="tx">TX</option> <option value="ut">UT</option> <option value="vt">VT</option> <option value="va">VA</option> <option value="wa">WA</option> <option value="wv">WV</option> <option value="wi">WI</option> <option value="wy">WY</option> </select>
              </div>
              <div class='large-12 columns' id='licenseplate'>  
                  <input id="licensenumber" name="licensenumber" placeholder="License Plate" type="text" required />
                    <small class="error">Please enter a valid license plate.</small>
              </div>
            </div>
            <div class='row'>
              <div class='large-12 columns'>
                <input class='inputReg' placeholder="Phone Number" name="phonenumber" id="phonenumber" type="tel" required />
                  <small class="error">Please enter a valid phone number.</small>
              </div>
            </div>
            <div class='row'>
              <div class="large-12 columns">
                  <input class='inputReg' id="name" name="name" placeholder="Name" type="text" required/>
                  <small class='error'>Please enter your name.</small>
              </div>
            </div>
            <div class='row'>
              <div class="large-12 columns">
                <input class='inputReg' placeholder="Email" name="email" id="email" type="email" required />
                  <small class="error">Please enter a valid email.</small>
              </div>
            </div>
            <div class='row'>
              <div class = "large-12 columns">
                <input class='inputReg' id = "password" name = "password" placeholder = "Password" type = "password" required/>
                  <small class="error">Password must be at least 8 characters and include at least one capital letter, one lower case letter and one number.</small>
              </div>
            </div>
            <div class='row'>
              <div class = "large-6 small-6 columns">
                <h5>How would you like to receive Wheeltalks?</h5>
              </div>
            </div>
            <div class='row'>
              <div class='large-6 small-6 columns'>
                <div class='large-4 columns' id="text">
                  <label for="text"><input type="checkbox" id="text" CHECKED ><span class="custom checkbox checked"></span> text</label>
                </div>
                <div class='large-4 columns' id='email'>  
                  <label for="email"><input type="checkbox" id="email" ><span class="custom checkbox"></span> email</label>
                </div>
                <div class='large-4 columns' id='Signin'>  
                  <label for="Sign in"><input type="checkbox" id="sign" ><span class="custom checkbox"></span> Sign in</label>
                </div>
              </div>
            </div>
            <div class='row'>
              <div class='large-6 small-6 columns'>
                <div id="button">
                  <input class='button' id='submitButton' type="submit" value="Submit" />
                </div>
              </div>
            </div>
    </div>
        </form>

      </div>
    </div>
   </div>
  </div>

  <!--FOOTER-->
  <div class='wrapper footer' id='signupFooter'>
    <div class='subWrapper'>
      <div class='row'>
        <div class='large-10 columns right' id='footer'>
          <ul>
              <li>
                <a href='#'>Help & FAQs</a>
