<!DOCTYPE html>
<!--[if IE 8]> 				 <html class="no-js lt-ie9" lang="en" > <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en" > <!--<![endif]-->

<head>
	<meta charset="utf-8">
  <meta name="viewport" content="width=device-width">
  <title>WheelTalks</title>
    
  <link rel="icon" type="image/png" href="img/logo.png">
  <link rel="stylesheet" href="css/foundation.css">
  <link rel='stylesheet' href='css/candy.css'>
  <link rel='stylesheet' href='css/cameleon.css'>
  <link rel='stylesheet' href='css/messageboard.css'>
  <link rel='stylesheet' href='fonts/general/stylesheets/general_foundicons_ie7.css'>
  <link rel='stylesheet' href='fonts/general/stylesheets/general_foundicons.css'>

  <script src="js/vendor/custom.modernizr.js"></script>


</head>

<body>

<!--HEADER, POSITION:FIXED, SMALL AND LARGE-->
  <div class='fixed contain-to-grid' id='navWrapper'>
    <nav class="top-bar" id='appNav'>
      <ul class="title-area">
        <li class="name">
          <a id='navName' href="#">WheelTalks</a>
        </li>
        <li class="toggle-topbar menu-icon">
          <a href="#"><span></span></a>
        </li>
      </ul>
      <section class="top-bar-section"> 
      <ul class="right">
        <li class="divider show-for-small" ></li>
        <li class="active"><a href="/?logout=true">Log Out</a></li>
      </ul>
      </section>
    </nav>
  </div>
  <div class='small-10 columns right'>
    <h2 class='header'>Inbox</h2>
  </div>
<!--MESSAGE:FROM-->
  <div class='small-10 columns small-centered receivedWrapper'>
    <div class='messageBox large-8 small-10 columns small-offset-1'>
      <div class='row messageHead'>
        <div class='from columns small-4'>
          <p>From: <span class='fromName' id='from12345'>Joe Smith</span></p>
        </div>
        <div class='date columns right small-4'>
          <p>Sent: <span class='dateRec' id='date12345'>1/1/2001</span></p>
        </div>
      </div>
      <div class='row contentWrapper'>
        <div class='messageContent columns'>
          <p><span class='contentRec' id='contentRec12345'>Welcome to WheelTalks!</span></p>
        </div>
      </div>
      <ul class='messageFunctions columns'>
        <li class='like'>
          <a href='#likepost12345'>Like <i class='foundicon-up-arrow functionImage'></i></a>
        </li>
        <li class='dislike'>
          <a href='#dislikepost12345'>Dislike <i class='foundicon-down-arrow functionImage'></i></a>
        </li>
        <li class='reply'>
          <a href='#replyPost12345'>Reply <i class='foundicon-add-doc functionImage'></i></a>
        </li>
        <li class='report'>
          <a href='#reportPost12345'>Report <i class='foundicon-flag functionImage'></i></a>
        </li>
        <li class='archive'>
          <a href='#archivePost12345'>Archive <i class='foundicon-trash functionImage'></i></a>
        </li>
      </ul>
    </div>
  </div>

  <div class='small-10 columns right'>
    <h2 class='header'>Outbox</h2>
  </div>
  <!--MESSAGE:SENT-->
    <div class='small-10 columns small-centered receivedWrapper'>
    <div class='messageBox large-8 small-10 columns small-offset-1'>
      <div class='row messageHead'>
        <div class='to columns small-4'>
          <p>To: <span class='toName' id='to12345'>Joe Smith</span></p>
        </div>
        <div class='date columns right small-4'>
          <p>Sent: <span class='dateSent' id='dateSent12345'>1/1/2001</span></p>
        </div>
      </div>
      <div class='row contentWrapper'>
        <div class='messageContent columns'>
          <p><span class='contentSent' id='contentSent12345'>Welcome to WheelTalks!</span></p>
        </div>
      </div>
      <ul class='messageFunctions columns'>
        <li class='archive'>
          <a href='#archivePost12345'>Archive <i class='foundicon-trash functionImage'></i></a>
        </li>
      </ul>
    </div>
  </div>


  <!--<script>
  document.write('<script src=' +
  ('__proto__' in {} ? 'js/vendor/zepto' : 'js/vendor/jquery') +
  '.js><\/script>')
  </script>-->

  <script src='js/vendor/jquery.js'></script>
  <script src="js/foundation.min.js"></script>
  <script src="js/raspberry.js"></script>
  
  <script>
    $(document).foundation();
  </script>


 
</body>
</html>
